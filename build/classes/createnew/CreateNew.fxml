<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!--Contains text field to write in name for new timetable and button for create new tables needed to be filled in
    order to generate new timetable.-->

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="474.0" prefWidth="535.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="createnew.CreateNewController">
   <children>
      <Label layoutX="58.0" layoutY="57.0" text="Please enter name of the new timetable:">
      </Label>
      <TextField fx:id="nameField" layoutX="58.0" layoutY="125.0" prefHeight="25.0" prefWidth="400.0">
      </TextField>
      <Button layoutX="58.0" layoutY="208.0" mnemonicParsing="false" onAction="#createTimetable" prefHeight="40.0" prefWidth="400.0" text="Create New Timetable">
      </Button>
      <Label fx:id="warningLabel" layoutX="58.0" layoutY="278.0" prefHeight="81.0" prefWidth="400.0" textFill="#e10606">
      </Label>
      <Button layoutX="338.0" layoutY="390.0" mnemonicParsing="false" onAction="#goBack" text="&lt;&lt;">
      </Button>
      <Button layoutX="398.0" layoutY="390.0" mnemonicParsing="false" onAction="#goForward" text="&gt;&gt;">
      </Button>
   </children>
</AnchorPane>
