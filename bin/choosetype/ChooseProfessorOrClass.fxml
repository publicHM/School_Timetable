<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="choosetype.ChooseProfessorOrClassController">
	<children>
      <Button minHeight="60.0" minWidth="400.0" mnemonicParsing="false" text="By Professor" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="30.0" left="60.0" right="60.0" top="30.0" />
         </VBox.margin>
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Button>
      <Button minHeight="60.0" minWidth="400.0" mnemonicParsing="false" text="By Class" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="30.0" left="60.0" right="60.0" top="30.0" />
         </VBox.margin>
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Button>
      <HBox alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="750.0">
         <children>
            <Button mnemonicParsing="false" text="&lt;&lt;">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="&gt;&gt;" textAlignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets right="50.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
